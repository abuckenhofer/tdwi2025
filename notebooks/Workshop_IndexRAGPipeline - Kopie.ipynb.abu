      "source": [
        "\n",
        "from pathlib import Path\n",
        "import pickle, faiss, requests, os, json\n",
        "\n",
        "# Mount Google¬†Drive (√ºberspringen, falls lokal)\n",
        "try:\n",
        "    from google.colab import drive\n",
        "    drive.mount('/content/drive', force_remount=False)\n",
        "    WORKDIR = Path('/content/drive/MyDrive/tdwi_workshop')\n",
        "except ModuleNotFoundError:\n",
        "    WORKDIR = Path.cwd() / 'tdwi_workshop'\n",
        "\n",
        "WORKDIR.mkdir(exist_ok=True)\n",
        "print(\"Arbeitsverzeichnis:\", WORKDIR.resolve())\n",
        "\n",
        "# Helper zum Download von Fallback-Dateien\n",
        "def fetch(url, path):\n",
        "    if not path.exists():\n",
        "        print('Lade Fallback:', url)\n",
        "        r = requests.get(url, timeout=30)\n",
        "        r.raise_for_status()\n",
        "        path.write_bytes(r.content)\n",
        "\n",
        "FALLBACK_BASE = 'https://raw.githubusercontent.com/your-org/tdwi-demo-artifacts/main/'\n",
        "\n",
        "# Beispiele: lade vorbereitete Objekte, falls vorhanden\n",
        "for fname in ['html_chunks.pkl', 'wiki.index']:\n",
        "    fetch(FALLBACK_BASE + fname, WORKDIR / fname)\n"
      ]
    },
