      "cell_type": "code",
      "source": [
        "import os, textwrap, numpy as np\n",
        "from openai import OpenAI\n",
        "from google.colab import userdata\n",
        "\n",
        "# --- key handling -----------------------------------------------------------\n",
        "api_key = userdata.get('OPENAI_API_KEY')\n",
        "if not api_key:\n",
        "    raise RuntimeError(\"OPENAI_API_KEY not set in Colab secrets!\")\n",
        "os.environ[\"OPENAI_API_KEY\"] = api_key\n",
        "\n",
        "client = OpenAI()                                      # ready to call\n",
        "\n",
        "# --- rest of ask_llm --------------------------------------------------------\n",
        "def ask_llm(question, k_ctx=1, model_name=\"gpt-3.5-turbo\"):\n",
        "    # 1) embed & normalise the question\n",
        "    q_vec = model.encode(\n",
        "        question,\n",
        "        convert_to_numpy=True,\n",
        "        normalize_embeddings=True            # ← OK\n",
        "    ).astype(\"float32\")                      # one array only\n",
        "\n",
        "    # 2) similarity search\n",
        "    D, I = index.search(q_vec.reshape(1, -1), k_ctx)\n",
        "    context = \"\\n\\n\".join(id_map[i][\"summary\"] for i in I[0])\n",
        "\n",
        "\n",
        "    prompt = textwrap.dedent(f\"\"\"\\\n",
        "        Use the context below to answer the question as clearly and concisely as possible.\n",
        "\n",
        "        ### Context\n",
        "        {context}\n",
        "\n",
        "        ### Question\n",
        "        {question}\n",
        "\n",
        "        ### Answer\n",
        "        \"\"\")\n",
        "\n",
        "    resp = client.chat.completions.create(\n",
        "        model=model_name,\n",
        "        messages=[{\"role\": \"user\", \"content\": prompt}],\n",
        "        temperature=0.3,\n",
        "    )\n",
        "    return resp.choices[0].message.content.strip()\n",
        "\n",
        "print(ask_llm(\"Who invented Python?\"))\n",
        "\n"
      ],
