      "source": [
        "model = SentenceTransformer('all-MiniLM-L6-v2')\n",
        "MY_UA = \"ColabVectorDemo/0.1 (me@example.com)\"  # <-  include email or URL\n",
        "wiki  = wikipediaapi.Wikipedia(user_agent=MY_UA, language='en')\n",
        "\n",
        "\n",
        "def add_articles(titles):\n",
        "    new_vecs, new_ids = [], []\n",
        "    for t in titles:\n",
        "        if t in (m['title'] for m in id_map.values()):\n",
        "            print('Skipping existing:', t); continue\n",
        "        page = wiki.page(t)\n",
        "        if not page.exists():\n",
        "            print('Not found:', t); continue\n",
        "        vec = model.encode(page.summary, convert_to_numpy=True,\n",
        "                           normalize_embeddings=True)\n",
        "        new_vecs.append(vec); new_ids.append(len(id_map))\n",
        "        id_map[len(id_map)] = {'title': page.title,\n",
        "                               'url': page.fullurl,\n",
        "                               'summary': page.summary}\n",
        "    if new_vecs:\n",
        "        index.add(np.vstack(new_vecs).astype('float32'))\n",
        "        print('Added', len(new_vecs), 'articles')\n",
        "\n",
        "default = [\"Python (programming language)\", \"Machine learning\",\n",
        "           \"Artificial intelligence\", \"Quantum mechanics\"]\n",
        "add_articles(default)"
      ]
    },
