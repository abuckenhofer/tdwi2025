      "source": [
        "import os, pickle, json, numpy as np, faiss, wikipediaapi\n",
        "from sentence_transformers import SentenceTransformer\n",
        "\n",
        "EMBEDDING_DIM = 384\n",
        "INDEX_PATH    = 'wiki.index'\n",
        "METADATA_PATH = 'id_map.pkl'"
      ]
    },
    {
      "cell_type": "markdown",
      "id": "baa1ee0a",
      "metadata": {
        "id": "baa1ee0a"
      },
      "source": [
        "## 1 · Create (or load) the FAISS index"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 3,
      "id": "2b27af56",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "2b27af56",
        "outputId": "32238abb-11a4-41c3-aead-c7995ef8a298"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Created new empty index.\n"
          ]
        }
      ],
      "source": [
        "if os.path.exists(INDEX_PATH) and os.path.exists(METADATA_PATH):\n",
        "    index = faiss.read_index(INDEX_PATH)\n",
        "    with open(METADATA_PATH, 'rb') as f: id_map = pickle.load(f)\n",
        "    print(f'Loaded existing index with {index.ntotal} vectors.')\n",
        "else:\n",
        "    index  = faiss.IndexFlatIP(EMBEDDING_DIM)      # cosine / dot‑product\n",
        "    id_map = {}\n",
        "    print('Created new empty index.')"
      ]
    },
