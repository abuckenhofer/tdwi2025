      "source": [
        "def search(q, k=4):\n",
        "    qv = model.encode(q, convert_to_numpy=True,\n",
        "                      normalize_embeddings=True).astype('float32')\n",
        "    D,I = index.search(qv.reshape(1,-1), k)\n",
        "    for s,i in zip(D[0], I[0]):\n",
        "        m = id_map[i]\n",
        "        print(f\"{m['title']}  (score={s:.4f})\\n{m['url']}\\n\")\n",
        "\n",
        "search(\"who invented python?\")"
      ]
    },
